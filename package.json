{
  "name": "reach-field-maps",
  "description": "REACH Field Maps",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Maxym Malynowsky <maxym.malynowsky@reach-initiative.org>",
  "scripts": {
    "build": "rm -rf .cache && rm -rf public && gatsby build && yarn manifest && yarn workbox",
    "build-deploy": "yarn download && yarn extract && gatsby build && yarn manifest && yarn workbox",
    "deploy": "yarn build && netlify deploy",
    "download": "curl https://fieldmaps-atlas.s3.eu-central-1.amazonaws.com/atlas-styles.tar.gz --output atlas-styles.tar.gz",
    "extract": "tar -xf atlas-styles.tar.gz --directory static && rm atlas-styles.tar.gz",
    "git": "yarn build && git push && git checkout master && git merge develop && git push && git checkout develop",
    "start": "rm -rf .cache && rm -rf public && gatsby develop",
    "start-prod": "yarn build && yarn serve",
    "network": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx}'",
    "lint": "tsc && eslint --fix 'src/**/*.{js,ts,jsx,tsx}'",
    "serve": "gatsby serve",
    "workbox": "node workbox.js",
    "manifest": "node manifest.js",
    "upgrade-all": "./yarn-upgrade-all.sh"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "d3-dsv": "^1.2.0",
    "gatsby": "^2.19.12",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-source-graphql": "^2.1.33",
    "gatsby-transformer-sharp": "^2.3.14",
    "graphql": "^14.6.0",
    "mapbox-gl": "^1.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "workbox-window": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@types/d3-dsv": "^1.0.36",
    "@types/mapbox-gl": "^1.7.0",
    "@types/react": "^16.9.19",
    "@types/react-helmet": "^5.0.15",
    "@types/workbox-window": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "1.x",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "workbox-build": "^5.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/impact-initiatives/reach-field-maps"
  }
}
